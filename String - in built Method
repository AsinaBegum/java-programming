//Program
public class Main {
    public static void main(String[] args) {
        String a="Hello Varshni";
        String b="World Asina";
        String x = "Varshni|Asina|Begum";
        System.out.println("Varshni".matches(x));
        System.out.println("Asina".matches(x));
        System.out.println("Java".matches(x));
        System.out.println(a.regionMatches(7,a,7,5));
    }
}
//Output
true
true
false
true


//Program
public class Main {
    public static void main(String[] args) {
        String x="World";
        String y="World Asina World";
        System.out.println(x.replace('l','p'));
        
        String a = "(?i)World";
        System.out.println(y.replaceAll(a,"Hii"));
        String b = "World";
        System.out.println(y.replaceFirst(b,"Hii"));
    }
}
//Output
Worpd
Hii Asina Hii
Hii Asina World


//Programpublic class Main {
    public static void main(String[] args) {
        String x=" Hello World";
        String y = "[,\\ .\\s]";
        String[] z = x.split(y);
        for(String s:z){
            System.out.println(s);

    }
}
}
//Output
Hello
World


//Program
import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a string: ");
        String x = sc.nextLine();
        String y = "[\\.\\s]";
        String[] z = x.split(y);
        for (String s : z) {
            System.out.println(s);
        }
    }
}
//Output
Enter a string: Asina Begum
Asina
Begum


//Program
import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a string: ");
        String x = sc.nextLine();
        String y = " ";
        String[] z = x.split(y);
        for (String s : z) {
            System.out.println(s);
        }
        System.out.println(x.concat(y));
    }
}
//Output
Enter a string: Asina Begum
Asina
Begum
Asina Begum 


//Program
import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        String x = "Hello";
        System.out.println(x.startsWith("Hello"));
        System.out.println(x.startsWith("World"));
    }
}
//Output
true
false


//Program
public class Main {
    public static void main(String[] args) {
        String x="Hello World";
        String a=(x.substring(0,5));
        System.out.println(a);
        String b=(x.substring(6,11));
        System.out.println(b);
        System.out.println(a.concat(" "+b));
	}
  }
//Output
Hello
World
Hello World


