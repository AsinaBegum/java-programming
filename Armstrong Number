//Armstrong number
import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int temp = n, sum = 0;
        while (n > 0) {
            int d = n % 10;
            sum = sum + d*d*d;
            n = n / 10;
        }

        if (sum == temp)
            System.out.println("Armstrong number");
        else
            System.out.println("Not Armstrong number");
    }
}

//OUTPUT:
153
Armstrong number
